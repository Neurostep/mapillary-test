{
  parser: "babel-eslint",
  extends: "airbnb",
  env: {
    "browser": true
  },
  rules: {
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "prefer-const": [0],
    "indent": ["error", 4],
    "comma-dangle": [0],
    "react/jsx-indent": [2, 4],
    "react/jsx-indent-props": [2, 4],
    "react/sort-comp": [0],
    "react/jsx-filename-extension": [0],
    "quotes": [2, "double", {"avoidEscape": true, "allowTemplateLiterals": true}]
  }
}
